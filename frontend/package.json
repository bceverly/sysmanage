    "test:coverage": "CI=true react-scripts test --passWithNoTests --coverage --verbose --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx --max-warnings 0 && stylelint 'src/**/*.css'"
  },
  "eslintConfig": {
    "extends": [
