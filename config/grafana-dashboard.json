{
  "dashboard": {
    "title": "SysManage Overview",
    "tags": ["sysmanage", "monitoring"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "30s",
    "panels": [
      {
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "id": 1,
        "targets": [
          {
            "expr": "rate(http_server_duration_milliseconds_count[5m])",
            "refId": "A"
          }
        ],
        "title": "HTTP Request Rate",
        "type": "graph",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        }
      },
      {
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "id": 2,
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_server_duration_milliseconds_bucket[5m]))",
            "refId": "A"
          }
        ],
        "title": "HTTP Request Duration (p95)",
        "type": "graph",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        }
      },
      {
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "id": 3,
        "targets": [
          {
            "expr": "http_server_active_requests",
            "refId": "A"
          }
        ],
        "title": "Active HTTP Requests",
        "type": "graph",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        }
      },
      {
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "id": 4,
        "targets": [
          {
            "expr": "rate(http_server_duration_milliseconds_sum[5m]) / rate(http_server_duration_milliseconds_count[5m])",
            "refId": "A"
          }
        ],
        "title": "Average Request Duration",
        "type": "graph",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        }
      },
      {
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        },
        "id": 5,
        "targets": [
          {
            "expr": "db_client_connections_usage",
            "refId": "A"
          }
        ],
        "title": "Database Connection Pool Usage",
        "type": "graph",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        }
      },
      {
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        },
        "id": 6,
        "targets": [
          {
            "expr": "up{job=\"sysmanage-backend\"}",
            "refId": "A"
          }
        ],
        "title": "SysManage Backend Status",
        "type": "stat",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        }
      }
    ]
  },
  "overwrite": false
}